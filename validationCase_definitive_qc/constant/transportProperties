FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

// Diffusivity
nu              [0 2 -1 0 0 0 0] 1e-05;

// Constants for teseLB
beta            [0 0 0 -1 0 0 0] 3e-3;
betaW           [0 0 0 0 0 0 0] 0; 
TRef            [0 0 0 1 0 0 0] 300;
wRef            [0 0 0 0 0 0 0] 0;
Pr              [0 0 0 0 0 0 0] 0.7;
Prt             [0 0 0 0 0 0 0] 0.85;
Sc              [0 0 0 0 0 0 0] 0.7;
Sct             [0 0 0 0 0 0 0] 0.7;
D               [0 2 -1 0 0 0 0] 1e-06; 

DTDtime         [0 0 -1 1 0 0 0] 0; // Rate of temperature change (K/s)
gradT           [0 -1 0 1 0 0 0] (0 0 0); // Temperature gradient (K/m)

linearTf        [0 0 0 1 0 0 0] 326;
liquidusSlope   [0 0 0 1 0 0 0] -5;
velModelCteA    [0 1 -1 0 0 0 0] 0;
velModelCteN    [0 0 0 0 0 0 0] 0;
enthalpyOfFusion [0 2 -2 0 0 0 0] 0;
specificHeat    [0 2 -2 -1 0 0 0] 1000;
physicalRho     [1 -3 0 0 0 0 0] 1000;
eutecticT       [0 0 0 1 0 0 0] 200;
eutecticW       [0 0 0 0 0 0 0] 1;
initialUndercooling [0 0 0 1 0 0 0] 8;
partitionK      [0 0 0 0 0 0 0] 1;
eutecticUndercooling [0 0 0 1 0 0 0] 0;

// True Thermodynamic Model Coefficients (Gibbs et al.)
ml0             [0 0 0 1 0 0 0] 503.02;     // Liquidus intersect
ml1             [0 0 0 1 0 0 0] -1.5372;    // Liquidus slope
ms0             [0 0 0 1 0 0 0] 503.15;     // Solidus intersect
ms1             [0 0 0 1 0 0 0] -4.439;     // Solidus slope

