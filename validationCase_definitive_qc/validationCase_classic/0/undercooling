FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      undercooling;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform -293;

boundaryField
{
    topAndBottom
    {
        type            calculated;
        value           uniform -293;
    }
    frontAndBack
    {
        type            calculated;
        value           uniform -293;
    }
    hot
    {
        type            calculated;
        value           uniform -307.75;
    }
    cold
    {
        type            calculated;
        value           uniform -288.15;
    }
}


