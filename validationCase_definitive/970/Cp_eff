FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "970";
    object      Cp_eff;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 -1 0 0 0];

internalField   uniform 201;

boundaryField
{
    topAndBottom
    {
        type            calculated;
        value           uniform 201;
    }
    frontAndBack
    {
        type            calculated;
        value           uniform 201;
    }
    hot
    {
        type            calculated;
        value           uniform 201;
    }
    cold
    {
        type            calculated;
        value           uniform 201;
    }
}


