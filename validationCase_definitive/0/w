FoamFile {
    format ascii;
    class volScalarField;
    location "0";
    object w;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [0 0 0 0 0 0 0];

internalField uniform 5;

boundaryField {
    frontAndBack {
        type zeroGradient;
    }

    topAndBottom {
        type zeroGradient;
    }

    hot {
        type zeroGradient;
    }

    cold {
        type zeroGradient;
    }
}


