FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      difft;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            zeroGradient;
    }

    topAndBottom
    {
        type            zeroGradient;
    }

    hot
    {
        type            zeroGradient;
    }

    cold
    {
        type            zeroGradient;
    }
}


