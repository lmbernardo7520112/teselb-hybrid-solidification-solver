FoamFile
{
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cellPoint;

setFormat raw;

sets
(
    lineX
    {
        type        face;
        axis        x;
        start       (0 1090 0);
        end         (0.076 1090 0);
        nPoints     35;
    }
);

fields
(
    T
    Gs
    w
);

